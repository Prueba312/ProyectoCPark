@model List<Park.Models.ViewModels.ListaUsuariosViewModels>

@{
    ViewBag.Title = "Datos";
}

    <h2>@ViewBag.Title</h2>

    <div class="row" style="text-align:right; padding:5px">
        <a href="~/DbPark/NuevoUsuario" class="btn btn-primary" >Nuevo</a>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table">
                <tr>
                    <td>#</td>
                    <td>Nombre</td>
                    <td>password</td>
                </tr>
                @foreach (var oElemento in Model)
                {
                    <tr>
                        <td>@oElemento.id</td>
                        <td>@oElemento.nombre</td>
                        <td>@oElemento.password</td>
                    </tr>
                
                }
            </table>
        </div>
    </div>
